@model HomeVM
@{
    ViewData["Title"] = "Index";
}

<div class="slider">
    <div class="col-12">
      <div class="container">
        <div class="row">
          <div class="main-slider-active">
            @foreach (Slider slider in Model.Sliders)
                    {
                        <div class="slider-area d-flex">
                            <div class="col-lg-6">
                                <div class="slide-content">
                                    <span class="offer">@slider.Offer</span>
                                    <h2 class="title">@slider.Title</h2>
                                    <p class="short-desc">
                                        @slider.Description
                                    </p>
                                    <div class="btn-wrap">
                                        <a class="btn" href="@slider.Link">Discover Now</a>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="inner-img">
                                    <img src="/assets/img/slider/@slider.Image" alt="" />
                                </div>
                            </div>
                        </div>
                    }
            
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="shipping-area">
    <div class="container">
      <div class="row shipping wrap">
        <div class="col-lg-4">
          <div class="shipping-item">
            <div class="shipping-img">
              <img src="/assets/img/icon/car.webp" alt="" />
            </div>
            <div class="shipping-content">
              <h2 class="title">Free Shipping</h2>
              <p class="short-desc">Capped at $319 per order</p>
            </div>
          </div>
        </div>
        <div class="col-lg-4">
          <div class="shipping-item">
            <div class="shipping-img">
              <img src="/assets/img/icon/card.webp" alt="" />
            </div>
            <div class="shipping-content">
              <h2 class="title">Safe Payment</h2>
              <p class="short-desc">With our payment gateway</p>
            </div>
          </div>
        </div>
        <div class="col-lg-4">
          <div class="shipping-item">
            <div class="shipping-img">
              <img src="/assets/img/icon/service.webp" alt="" />
            </div>
            <div class="shipping-content">
              <h2 class="title">Best Services</h2>
              <p class="short-desc">Friendly & Supper Services</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <section id="ourproduct-area">
    <div class="ourproduct-area">
      <div class="container">
        <div class="title-section">
          <h2 class="title">OUR PRODUCTS</h2>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
              <li class="nav-item" role="presentation">
                <a class="nav-link active" id="featured-tab" data-bs-toggle="tab" data-bs-target="#featured-tab-pane"
                  role="tab" aria-controls="featured-tab-pane" aria-selected="true">Featured</a>
              </li>
              <li class="nav-item" role="presentation">
                <a class="nav-link" id="profile-tab" data-bs-toggle="tab" data-bs-target="#bestseller-tab-pane"
                  role="tab" aria-controls="bestseller-tab-pane" aria-selected="false">Bestseller</a>
              </li>
              <li class="nav-item" role="presentation">
                <a class="nav-link" id="latest-tab" data-bs-toggle="tab" data-bs-target="#latest-tab-pane" role="tab"
                  aria-controls="latest-tab-pane" aria-selected="false">Latest</a>
              </li>
            </ul>
            <div class="tab-content" id="myTabContent">
              <div class="tab-pane fade show active" id="featured-tab-pane" role="tabpanel"
                aria-labelledby="featured-tab" tabindex="0">
                            @await Html.PartialAsync("_ProductListPartial",Model.Featured)
              </div>

              <div class="tab-pane fade" id="bestseller-tab-pane" role="tabpanel" aria-labelledby="bestseller-tab"
                tabindex="0">
                            @await Html.PartialAsync("_ProductListPartial",Model.BestSeller)
              </div>

              <div class="tab-pane fade" id="latest-tab-pane" role="tabpanel" aria-labelledby="latest-tab" tabindex="0">
                @await Html.PartialAsync("_ProductListPartial",Model.Latest)
              </div>

              
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section id="banner">
    <div class="banner-area">
      <div class="container">
        <div class="row g-4">
          <div class="col-lg-8">
            <div class="banner-item img-hover-effect">
